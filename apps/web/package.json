{
	"name": "web",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "sst bind vite dev",
		"build": "vite build",
		"package": "vite package",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
