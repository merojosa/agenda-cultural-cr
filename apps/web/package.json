{
	"name": "web",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "sst bind vite dev",
		"build": "vite build",
		"package": "vite package",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --ignore-path=../../.prettierignore . && eslint \"src\"",
		"format": "prettier --write --ignore-path=../../.prettierignore .",
		"deploy:dev": "sst deploy",
		"deploy:prod": "sst deploy --stage prod",
		"sync": "svelte-kit sync",
		"typecheck": "tsc --noEmit"
	},
	"devDependencies": {
		"@sveltejs/kit": "^1.27.6",
		"@types/luxon": "^3.3.4",
		"autoprefixer": "^10.4.16",
		"aws-cdk-lib": "2.101.1",
		"constructs": "10.2.69",
		"db-schema": "*",
		"postcss": "^8.4.31",
		"sst": "2.36.1",
		"svelte": "^4.2.5",
		"svelte-check": "^3.6.0",
		"svelte-kit-sst": "2.36.1",
		"tailwindcss": "^3.3.5",
		"tsconfig": "*",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.5.0"
	},
	"dependencies": {
		"bits-ui": "^0.9.8",
		"clsx": "^2.0.0",
		"drizzle-orm": "^0.29.0",
		"lucide-svelte": "^0.292.0",
		"luxon": "^3.4.4",
		"postgres": "^3.4.3",
		"tailwind-merge": "^2.0.0",
		"tailwind-variants": "^0.1.18"
	},
	"type": "module"
}
