		"db-schema": "*",
		"postcss": "^8.4.31",
		"svelte": "^4.2.1",
		"svelte-check": "^3.5.2",
		"tailwindcss": "^3.3.3",
		"tsconfig": "*",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.4.11",
		"sst": "^2.28.1",
		"aws-cdk-lib": "2.95.1",
		"constructs": "10.2.69",
		"svelte-kit-sst": "^2.28.1"
	},
	"dependencies": {
