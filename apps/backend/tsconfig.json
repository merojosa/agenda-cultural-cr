{
	"extends": "tsconfig/base.json",
	"compilerOptions": {
		"outDir": "dist",
		"module": "CommonJS",
		"target": "es2022",
		"paths": {
			"#scraping/*": ["./src/scraping/*"],
			"#services/*": ["./src/services/*"],
			"#utils/*": ["./src/utils/*"]
		}
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules"]
}
