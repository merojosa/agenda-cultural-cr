{
  "name": "backend",
  "version": "0.3.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "start:dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "rm -rf dist && esbuild src/index.ts --bundle --platform=node --packages=external --target=node18 --outdir=dist",
    "deploy:dev": "yarn build && sls deploy --stage dev"
  },
  "dependencies": {
    "luxon": "^3.4.0",
    "puppeteer-core": "^21.0.3"
  },
  "devDependencies": {
    "@sparticuz/chromium": "^115.0.0",
    "@types/luxon": "^3.3.1",
    "@types/node": "^18.17.4",
    "esbuild": "^0.19.1",
    "serverless": "^3.34.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  }
}
