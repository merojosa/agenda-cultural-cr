		"drizzle-kit": "^0.19.13",
		"drizzle-orm": "^0.28.6",
		"postgres": "^3.3.5",
		"tsconfig": "*",
		"tsx": "^3.12.8",
		"typescript": "^5.2.2"
	}
}
