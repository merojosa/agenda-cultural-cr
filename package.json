		"deploy:dev": "turbo run deploy:dev"
	},
	"devDependencies": {
		"aws-cdk-lib": "2.110.1",
		"constructs": "10.3.0",
		"eslint": "^8.51.0",
		"eslint-config-custom": "*",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"prettier-plugin-tailwindcss": "^0.4.1",
		"sst": "2.36.7",
		"svelte-kit-sst": "2.36.7",
		"turbo": "^1.10.16"
	},
	"workspaces": {
		"packages": [
			"packages/*",
			"apps/*"
		]
	}
}
