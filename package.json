{
	"name": "agenda-cultural-cr",
	"private": true,
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev",
		"lint": "turbo run lint",
		"format": "prettier --write .",
		"backend:run": "turbo run backend#run",
		"backend:deploy:dev": "turbo run backend#deploy:dev",
		"db:generate": "turbo run db#generate",
		"db:deploy:development": "yarn workspace db deploy:development"
	},
	"devDependencies": {
		"eslint": "^8.28.0",
		"eslint-config-custom": "*",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"turbo": "^1.10.13"
	},
	"workspaces": {
		"packages": [
			"packages/*",
			"apps/*"
		],
		"nohoist": [
			"backend/**"
		]
	}
}
