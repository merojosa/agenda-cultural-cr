{
	"name": "agenda-cultural-cr",
	"private": true,
	"scripts": {
		"dev": "turbo run dev",
		"lint": "turbo run lint",
		"test": "turbo run test",
		"typecheck": "turbo run typecheck",
		"format": "prettier --write .",
		"db:generate": "turbo run db#generate",
		"db:deploy:dev": "yarn workspace db deploy:dev",
		"db:seed:dev": "turbo run db#seed:dev",
