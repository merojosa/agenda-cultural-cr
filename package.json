{
	"name": "agenda-cultural-cr",
	"private": true,
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev",
		"lint": "turbo run lint",
		"test": "turbo run test",
		"typecheck": "turbo run typecheck",
		"format": "prettier --write .",
		"backend:run": "turbo run backend#run",
		"backend:deploy:dev": "turbo run backend#deploy:dev",
		"db:generate": "turbo run db#generate",
		"db:deploy:dev": "yarn workspace db deploy:dev",
		"db:seed:dev": "turbo run db#seed:dev",
		"deploy:dev": "turbo run sst:deploy:dev && sst deploy --stage dev"
	},
	"devDependencies": {
		"aws-cdk-lib": "2.101.1",
		"constructs": "10.2.69",
		"eslint": "^8.51.0",
		"eslint-config-custom": "*",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"prettier-plugin-tailwindcss": "^0.4.1",
		"sst": "2.36.1",
		"svelte-kit-sst": "2.36.1",
		"turbo": "^1.10.16"
	},
	"workspaces": {
		"packages": [
			"packages/*",
			"apps/*"
		],
		"nohoist": [
			"backend/**"
		]
	}
}
